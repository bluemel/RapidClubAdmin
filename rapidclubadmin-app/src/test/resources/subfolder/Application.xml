<?xml version="1.0" encoding="ISO-8859-1" standalone="yes"?>
<bean type="org.rapidbeans.presentation.config.ConfigApplication"
	name="Test"
	rootpackage="org.rapidbeans"
	applicationclass="org.rapidbeans.presentation.TestClient"
	guitype="swing"
	lookandfeel="windows"
	defaultlocale="en"
	>
	<locale
		id="de"
		name="de"
		/>
	<locale
		id="en"
		name="en"
		/>
	<locale
		id="fr"
		name="fr"
		/>
	<mainwindow
		name="mainwindow"
		width="800"
		height="500"
		>
		<menubar
			name="menubar"
			>
			<menu
				name="file"
				>
				<menuentry
					type="org.rapidbeans.presentation.config.ConfigMenuItem"
					name="load"
					/>
				<menuentry
					type="org.rapidbeans.presentation.config.ConfigMenuItem"
					name="save"
					/>
				<menuentry
					type="org.rapidbeans.presentation.config.ConfigMenuItem"
					name="quit"
					>
					<action
						classname="org.rapidbeans.service.ActionQuit"
						/>
				</menuentry>
			</menu>
			<menu
				name="edit"
				>
				<menuentry
					type="org.rapidbeans.presentation.config.ConfigMenuItem"
					name="view"
					/>
				<menuentry
					type="org.rapidbeans.presentation.config.ConfigSubmenu"
					name="insert"
					>
					<menuentry
						type="org.rapidbeans.presentation.config.ConfigMenuItem"
						name="object1"
						/>
					<menuentry
						type="org.rapidbeans.presentation.config.ConfigMenuItem"
						name="object2"
						/>
				</menuentry>
			</menu>
			<menu
				name="test"
				>
				<menuentry
					type="org.rapidbeans.presentation.config.ConfigMenuItem"
					name="test1"
					/>
				<menuentry
					type="org.rapidbeans.presentation.config.ConfigSubmenu"
					name="test2"
					>
					<menuentry
						type="org.rapidbeans.presentation.config.ConfigMenuItem"
						name="test21"
						/>
					<menuentry
						type="org.rapidbeans.presentation.config.ConfigSubmenu"
						name="test22"
						>
						<menuentry
							type="org.rapidbeans.presentation.config.ConfigMenuItem"
							name="test221"
							/>
						<menuentry
							type="org.rapidbeans.presentation.config.ConfigMenuItem"
							name="test222"
							/>
					</menuentry>
					<menuentry
						type="org.rapidbeans.presentation.config.ConfigMenuItem"
						name="test23"
						/>
				</menuentry>
			</menu>
		</menubar>
	</mainwindow>
</bean>
